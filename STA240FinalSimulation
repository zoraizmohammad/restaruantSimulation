\n","simulate_monthly_arrivals <- function(days, lambda, start_hour, end_hour) {\n","  all_arrivals <- c()\n","\n","  for (i in 1:days) {\n","    daily_arrivals <- generate_arrival_times(lambda, start_hour, end_hour)\n","    all_arrivals <- c(all_arrivals, daily_arrivals)\n","  }\n","\n","  # Convert to hourly buckets\n","  arrival_hours <- floor(all_arrivals / 60)\n","  arrival_df <- data.frame(Hour = arrival_hours)\n","\n","  # Aggregate to calculate average arrivals per hour\n","  average_arrivals <- arrival_df %>%\n","    group_by(Hour) %>%\n","    summarize(Average_Arrivals = n() / days)\n","\n","  return(average_arrivals)\n","}\n","\n","# Parameters\n","days <- 30           # Number of days to simulate\n","lambda <- 5          # Average arrival rate per hour\n","start_hour <- 10     # Start of operational hours (10 AM)\n","end_hour <- 22       # End of operational hours (10 PM)\n","\n","# Simulate arrivals\n","average_arrivals <- simulate_monthly_arrivals(days, lambda, start_hour, end_hour)\n","\n","# Visualization\n","ggplot(average_arrivals, aes(x = Hour, y = Average_Arrivals)) +\n","  geom_bar(stat = \"identity\", fill = \"steelblue\", alpha = 0.8) +\n","  scale_x_continuous(breaks = seq(start_hour, end_hour, 1), labels = sprintf(\"%02d:00\", seq(start_hour, end_hour, 1) %% 24)) +\n","  labs(\n","    title = \"Average Customer Arrivals Per Hour Over a Month\",\n","    x = \"Hour of the Day\",\n","    y = \"Average Number of Customers\"\n","  ) +\n","  theme_minimal()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":680},"id":"v1wd6DYqa9LU","executionInfo":{"status":"ok","timestamp":1733546473327,"user_tz":300,"elapsed":1333,"user":{"displayName":"Mohammad Zoraiz","userId":"07318812600179621858"}},"outputId":"093e1414-24ed-45f3-acc0-c0e625f0d772"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stderr","text":["\n","Attaching package: ‘dplyr’\n","\n","\n","The following objects are masked from ‘package:stats’:\n","\n",