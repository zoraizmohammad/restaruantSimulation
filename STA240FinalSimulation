3bo0KE//vGPpDoAeYoeOwAAAEHQYwcAACAI\ngh0AAIAgCHYAAACCINgBAAAIgmAHAAAgCIIdAACAIAh2AAAAgiDYAQAACOL/Ao4RO4ENGKZf\nAAAAAElFTkSuQmCC"},"metadata":{"image/png":{"width":420,"height":420}}}]},{"cell_type":"code","source":["library(ggplot2)\n","\n","# Function to simulate restaurant service with VIP priority\n","simulate_vip_priority <- function(arrival_times, service_times, vip_prob = 0.5, start_hour = 10) {\n","  n <- length(arrival_times)\n","\n","  # Assign VIP status (1 for VIP, 0 for regular)\n","  is_vip <- rbinom(n, 1, vip_prob)\n","  vip_indices <- which(is_vip == 1)\n","  regular_indices <- which(is_vip == 0)\n","\n","  # Separate VIP and regular customers\n","  vip_arrivals <- arrival_times[vip_indices]\n","  vip_services <- service_times[vip_indices]\n","  regular_arrivals <- arrival_times[regular_indices]\n","  regular_services <- service_times[regular_indices]\n","\n","  # Merge VIPs and regular customers into a priority queue\n","  all_arrivals <- c(vip_arrivals, regular_arrivals)\n","  all_services <- c(vip_services, regular_services)\n","  priorities <- c(rep(\"VIP\", length(vip_arrivals)), rep(\"Regular\", length(regular_arrivals)))\n","  priority_order <- order(all_arrivals, priorities) # VIPs served first if arrival time is the same\n","\n","  ordered_arrivals <- all_arrivals[priority_order]\n","  ordered_services <- all_services[priority_order]\n","  ordered_priorities <- priorities[priority_order]\n","\n","  # Simulate the prioritized queue\n","  n <- length(ordered_arrivals)\n","  table_free_time <- 0 # Single chef's free time\n","  start_times <- numeric(n)\n","  end_times <- numeric(n)\n","  waiting_times <- numeric(n)\n","\n","  for (i in 1:n) {\n","    if (ordered_arrivals[i] >= table_free_time) {\n","      start_times[i] <- ordered_arrivals[i]\n","    } else {\n","      start_times[i] <- table_free_time\n","    }\n","    waiting_times[i] <- start_times[i] - ordered_arrivals[i]\n","    end_times[i] <- start_times[i] + ordered_services[i]\n","    table_free_time <- end_times[i]\n","  }\n","\n","  # Format times\n","  format_time <- function(time_in_minutes) {\n","    format(as.POSIXct((start_hour * 60 + time_in_minutes) * 60, origin = \"1970-01-01\", tz = \"UTC\"), \"%I:%M %p\")\n","  }\n","  formatted_arrivals <- sapply(ordered_arrivals, format_time)\n","  formatted_starts <- sapply(start_times, format_time)\n","  formatted_ends <- sapply(end_times, format_time)\n","\n","  # Create raw data\n","  raw_data <- data.frame(\n","    Customer = 1:n,\n","    Arrival = formatted_arrivals,\n","    Start = formatted_starts,\n","    End = formatted_ends,\n","    Waiting_Time = waiting_times,\n","    Priority = ordered_priorities\n","  )\n","\n","  # Return the raw data and results\n","  return(list(raw_data = raw_data, waiting_times = waiting_times, priorities = ordered_priorities))\n","}\n","\n","# Function for monthly simulation\n","simulate_monthly_vip_priority <- function(days, customers_per_day, vip_prob = 0.2, start_hour = 10) {\n","  all_waiting_times <- numeric()\n","  all_priorities <- character()\n","\n","  for (day in 1:days) {\n","    set.seed(day) # Ensure reproducibility for each day\n","    arrival_times <- cumsum(rexp(customers_per_day, rate = 5 / 60)) # Poisson process for arrivals\n","    service_times <- rexp(customers_per_day, rate = 1 / 10) * 60 # Service times in minutes\n","\n","    # Simulate daily VIP priority scenario\n","    daily_result <- simulate_vip_priority(arrival_times, service_times, vip_prob, start_hour)\n","    all_waiting_times <- c(all_waiting_times, daily_result$waiting_times)\n","    all_priorities <- c(all_priorities, daily_result$priorities)\n","  }\n","\n","  # Create data frame for visualization and analysis\n","  waiting_times_df <- data.frame(\n","    Waiting_Time = all_waiting_times,\n","    Priority = all_priorities\n","  )\n","\n","  # Calculate summary statistics\n","  avg_waiting_time_overall <- mean(all_waiting_times)\n","  avg_waiting_time_vip <- mean(all_waiting_times[all_priorities == \"VIP\"])\n","  avg_waiting_time_regular <- mean(all_waiting_times[all_priorities == \"Regular\"])\n","\n","  cat(\"Monthly Summary Statistics:\\n\")\n","  cat(\"Average Waiting Time (Overall):\", avg_waiting_time_overall, \"minutes\\n\")\n","  cat(\"Average Waiting Time (VIP):\", avg_waiting_time_vip, \"minutes\\n\")\n","  cat(\"Average Waiting Time (Regular):\", avg_waiting_time_regular, \"minutes\\n\")\n","\n","  # Visualization: Waiting times distribution\n","  ggplot(waiting_times_df, aes(x = Waiting_Time, fill = Priority)) +\n","    geom_histogram(bins = 30, position = \"dodge\", alpha = 0.7, color = \"black\") +\n","    labs(\n","      title = \"Distribution of Waiting Times Over the Month\",\n","      x = \"Waiting Time (minutes)\",\n","      y = \"Frequency\",\n","      fill = \"Priority\"\n","    ) +\n","    theme_minimal()\n","}\n","\n","# Simulate for one month (30 days)\n","days_in_month <- 30\n","customers_per_day <- 30\n","\n","simulate_monthly_vip_priority(days_in_month, customers_per_day, vip_prob = 0.2, start_hour = 10)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":506},"id":"8vj7lCOy3dBN","executionInfo":{"status":"ok","timestamp":1733553986230,"user_tz":300,"elapsed":1512,"user":{"displayName":"Mohammad Zoraiz","userId":"07318812600179621858"}},"outputId":"df5ebc27-a35a-4039-a1c7-8c1ec10b7a29"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Monthly Summary Statistics:\n","Average Waiting Time (Overall): 8313.075 minutes\n","Average Waiting Time (VIP): 8444.593 minutes\n","Average Waiting Time (Regular): 8282.003 minutes\n"]},{"output_type":"display_data","data":{"text/plain":["plot without title"],"image/png":"iVBORw0KGgoAAAANSUhEUgAAA0gAAANICAIAAAByhViMAAAACXBIWXMAABJ0AAASdAHeZh94\nAAAgAElEQVR4nOzdd2AU1f738TPb0gMBwqVFmvQqCJemgDQRBBGxAEr7KVFRuAoKAgJBLCAG\nRS71qlyKXkWk2hEbUlSaiuAVFEMJCCE9my0zzx9H9tmbnV02PTu8X3+RM3NmvnNmZvfDzOyu\nommaAAAAQOgzlXcBAAAAKBkEOwAAAIMg2AEAA