nz3nvv0ck3btwoKipaXFz87rvvfvrpp9zc3Hfeeeezzz5b30sAgI0Da+wA\nAP6BRqNpbm7OyspiGKaurq6xsdHlcn344YcMwyQkJJSXlzc1NdGZ9fX1ycnJ169fF4lEDMOU\nlJRotdrTp0/X1NQIBIJ1vAQA2CAwYgcA8A/EYjFFdQzDKBQKhmF27ty58q8KhcLpdNrtdoPB\nMDQ0VFZWxrKs59/27dtnt9uHh4fXp+gAsMEgsAMA+AexsbErf4eGhjIMExMTE3CE4zitVssw\nzJdffilcpbq6mmEYjUbzpAsNABsSpmIBAB6nt99+u6qqKuBgamrquhQGADYaBHYAAI9HYmIi\nwzAcx1GWYwCAJw9TsQAAj0d0dPSOHTuamppsNtvKwStXrpw8eXJpaWkdCwYAGwcCOwCAx+b8\n+fMul6ugoODKlSvt7e2nTp2qrKycm5vj8TA9AgBPAvoaAIDHpqCgoKur68yZM0ePHvV4PMnJ\nyefOnautrV3vcgHARoGfFAMAAAAIEpiKBQAAAAgSCOwAAAAAggQCOwAAAIAggcAOAAAAIEgg\nsAMAAAAIEgjsAAAAAIIEAjsAAACAIIHADgAAACBIILADAAAACBII7AAAAACCBAI7AAAAgCCB\nwA4AAAAgSPwLWp18bsr0r0IAAAAASUVORK5CYII="},"metadata":{"image/png":{"width":420,"height":420}}}]},{"cell_type":"code","source":["simulate_and_visualize_two_tables <- function(lambda, start_hour, end_hour, service_shape, service_scale, days = 30) {\n","  library(ggplot2)\n","  library(dplyr)\n","\n","  # Generate customer arrival times for a single day\n","  generate_arrival_times <- function(lambda, start_hour, end_hour) {\n","    total_minutes <- (end_hour - start_hour) * 60\n","    arrivals <- cumsum(rexp(ceiling(lambda * total_minutes / 60), rate = lambda / 60))\n","    arrivals <- arrivals[arrivals <= total_minutes] # Keep arrivals within operational hours\n","    return(arrivals)\n","  }\n","\n","  # Simulate service times for customers\n","  generate_service_times <- function(n, shape, scale) {\n","    rgamma(n, shape = shape, scale = scale)\n","  }\n","\n","  # Simulate restaurant operations with two tables\n","  simulate_two_tables <- function(arrival_minutes, service_times) {\n","    n <- length(arrival_minutes)\n","    start_times <- numeric(n)\n","    end_times <- numeric(n)\n","    waiting_times <- numeric(n)\n","\n","    table_free_time <- c(0, 0) # Two tables, initially free at time 0\n","\n","    for (i in 1:n) {\n","      # Find the earliest free table\n","      next_free_table <- which.min(table_free_time)\n","\n","      if (arrival_minutes[i] >= table_free_time[next_free_table]) {\n","        # Customer is served immediately\n","        start_times[i] <- arrival_minutes[i]\n","      } else {\n","        # Customer waits\n","        start_times[i] <- table_free_time[next_free_table]\n","      }\n","\n","      # Calculate waiting time\n","      waiting_times[i] <- start_times[i] - arrival_minutes[i]\n","\n","      # Calculate end time and update the table's free time\n","      end_times[i] <- start_times[i] + service_times[i]\n","      table_free_time[next_free_table] <- end_times[i]\n","    }\n","\n","    return(list(\n","      start_times = start_times,\n","      end_times = end_times,\n","      waiting_times = waiting_times\n","    ))\n","  }\n","\n","  # Simulate customer arrivals and service times for one day\n","  simulate_one_day <- function(lambda, start_hour, end_hour, service_shape, service_scale) {\n","    arrival_minutes <- generate_arrival_times(lambda, start_hour, end_hour)\n","    service_times <- generate_service_times(length(arrival_minutes), service_shape, service_scale)\n","\n","    result <- simulate_two_tables(arrival_minutes, service_times)\n","\n","    # Convert minutes to AM/PM time for readability\n","    arrival_times <- format(as.POSIXct((start_hour * 60 + arrival_minutes) * 60, origin = \"1970-01-01\", tz = \"UTC\"), \"%I:%M %p\")\n","    start_times <- format(as.POSIXct((start_hour * 60 + result$start_times) * 60, origin = \"1970-01-01\", tz = \"UTC\"), \"%I:%M %p\")\n","    end_times <- format(as.POSIXct((start_hour * 60 + result$end_times) * 60, origin = \"1970-01-01\", tz = \"UTC\"), \"%I:%M %p\")\n","\n","    data.frame(\n","      Arrival_Time = arrival_times,\n","      Start_Time = start_times,\n","      End_Time = end_times,\n","      Waiting_Time = result$waiting_times,\n","      Service_Time = service_times\n","    )\n","  }\n","\n","  # Simulate for a month (30 days)\n","  simulate_month <- function(lambda, start_hour, end_hour, service_shape, service_scale, days) {\n","    daily_data <- lapply(1:days, function(day) {\n","      simulate_one_day(lambda, start_hour, end_hour, service_shape, service_scale)\n","    })\n","    month_data <- do.call(rbind, daily_data)\n","    month_data$Day <- rep(1:days, sapply(daily_data, nrow))\n","    return(month_data)\n","  }\n","\n","  # Simulate a month's data\n","  month_data <- simulate_month(lambda, start_hour, end_hour, service_shape, service_scale, days)\n","\n","  # Add Arrival Hour for plotting\n","  month_data$Arrival_Hour <- sapply(month_data$Arrival_Time, function(at) {\n","    as.numeric(format(as.POSIXct(at, format = \"%I:%M %p\", tz = \"UTC\"), \"%H\")) +\n","      as.numeric(format(as.POSIXct(at, format = \"%I:%M %p\", tz = \"UTC\"), \"%M\")) / 60\n","  })\n","\n","  # Visualization 1: Average customer arrival times over the course of a day\n","  arrival_hourly_avg <- month_data %>%\n","    dplyr::group_by(Arrival_Hour) %>%\n","    dplyr::summarize(Avg_Customers = n() / days)\n","\n","  plot1 <- ggplot(arrival_hourly_avg, aes(x = Arrival_Hour, y = Avg_Customers)) +\n","    geom_line(color = \"blue\") +\n","    labs(\n","      title = \"Average Customer Arrival Times Over a Day\",\n","      x = \"Hour of the Day\",\n","      y = \"Average Number of Arrivals\"\n","    ) +\n","    theme_minimal()\n","\n","  # Visualization 2: Service time distribution\n","  plot2 <- ggplot(month_data, aes(x = Service_Time)) +\n","    geom_histogram(binwidth = 0.1, fill = \"orange\", color = \"black\", alpha = 0.7) +\n","    labs(\n","      title = \"Service Time Distribution Over a Month\",\n","      x = \"Service Time (minutes)\",\n","      y = \"Frequency\"\n","    ) +\n","    theme_minimal()\n","\n","  # Display plots\n","  print(plot1)\n","  print(plot2)\n","\n","  # Return raw data and summaries\n","  list(\n","    raw_data = month_data,\n","    avg_arrivals_per_hour = arrival_hourly_avg\n","  )\n","}\n","\n","# Parameters for simulation\n","lambda <- 5              # Average 5 customers per hour\n","start_hour <- 10         # Operational start time (10 AM)\n","end_hour <- 22           # Operational end time (10 PM)\n","service_shape <- 2       # Shape parameter for Gamma distribution (service times)\n","service_scale <- 0.2     # Scale parameter for Gamma distribution (service times)\n","\n","# Run the simulation and visualization\n","result <- simulate_and_visualize_two_tables(lambda, start_hour, end_hour, service_shape, service_scale)\n"],"metadata":{"id":"RBtTceEdhbh5","executionInfo":{"status":"ok","timestamp":1733548341592,"user_tz":300,"elapsed":1026,"user":{"displayName":"Mohammad Zoraiz","userId":"07318812600179621858"}},"outputId":"add0a6ce-acc1-4d0e-81af-f618be27718b","colab":{"base_uri":"https://localhost:8080/","height":857}},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["plot without title"],"image/png":"iVBORw0KGgoAAAANSUhEUgAAA0gAAANICAIAAAByhViMAAAACXBIWXMAABJ0AAASdAHeZh94\nAAAgAElEQVR4nOydd7wVxd3/v6feDly8qBiNBQ1YEo0aA/jYgxrR+BgfTVCjEX8RDZH4GAsa\nYkGjJjEvMcQYa2yxF6wRY8TYsIv6GDGKioiFALecW0/b3x9zWZezu7OzfWbP5/0Hr8ue2Znv\nzM7MfuY7ZVOaphEAAAAAAFCfdNwGAAAAAACAYICwAwAAAABICBB2AAAAAAAJAcIOAAAAACAh\nQNgBAAAAACQECDsAAAAAgIQAYQcAAAAAkBAg7AAIktmzZ6dSqT//+c8BxjlnzpxUKvXHP/7R\nZzxh2AYAAEAqkizs+vr6Ro4cmUql9tlnn7htiYFVq1ZdcMEFkyZNGjNmTENDw6abbjp58uTL\nLrvsP//5TxjJ3XLLLQ8++GAYMUdAgFWlvb198803b2trC8QwEc4///yUE6NGjYrFNg988cUX\nF1xwwe67777hhhvm8/kNNtjgW9/61uzZsz/66KO4TXPNkiVLah5Ea2vrVltt9T//8z833nhj\nf39/3AYCABJIKsFfnrjmmmtmzJix1VZbffDBB0uXLh0/fnzcFkXHzTfffPLJJ/f39+dyuR13\n3LG9vX3lypXvv/9+sVhsb2+/44479t9//2BTHDt27NSpU6+77rpgo40GyavKnDlzfv3rX8+f\nP/9nP/uZ+dc77rjjjjvu0P/72WefvfTSS2PGjJk8ebJ+saWl5a9//WsUtvrjmmuuOfXUUwcG\nBvL5/Ne//vUNNtigu7v7jTfeGBwczGazc+fOPfvss+O20QVLliz55je/2draOnXqVHalt7eX\n1TFN077yla/85S9/mTJlSrxGAgCShpZcdtppp7a2tgceeICITj311LjNiY7bb7+diNLp9C9/\n+cvu7m79+hdffDFr1iwiymQyr732WoApLlu2jIhOOOGEAOOMEsmryi9/+Usimj9/vkjg+++/\nn4j222+/sK0KnGuuuYaIstnsRRdd1NPTo1/v7++//PLLGxsbi