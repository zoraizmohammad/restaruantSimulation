n","    labs(\n","      title = \"Chef Utilization\",\n","      x = \"Status\",\n","      y = \"Percentage (%)\",\n","      subtitle = \"Breakdown of Busy vs. Idle Time\"\n","    ) +\n","    scale_fill_manual(values = c(\"Busy\" = \"green\", \"Idle\" = \"gray\")) +\n","    theme_minimal() +\n","    theme(\n","      legend.position = \"none\",\n","      plot.title = element_text(hjust = 0.5, size = 16),\n","      plot.subtitle = element_text(hjust = 0.5, size = 12)\n","    )\n","}\n","\n","# Example Chef Utilization Value\n","chef_utilization <- 75  # Example value: 75% busy\n","\n","# Plot the utilization\n","utilization_plot <- plot_chef_utilization(chef_utilization)\n","print(utilization_plot)\n"],"metadata":{"id":"SND5f0JmFQoo","colab":{"base_uri":"https://localhost:8080/","height":437},"executionInfo":{"status":"ok","timestamp":1732943815995,"user_tz":300,"elapsed":338,"user":{"displayName":"Johannes Arizie","userId":"09686860358701013863"}},"outputId":"0dda417f-ce5f-4b92-e729-6abf969d77a6"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["plot without title"],"image/png":"iVBORw0KGgoAAAANSUhEUgAAA0gAAANICAIAAAByhViMAAAACXBIWXMAABJ0AAASdAHeZh94\nAAAgAElEQVR4nOzdeUDUdf748feH4RbkEPBExAuvANG8FQ/Mc6lIK3M18QpXswstazfT/WZb\n2Kprlmkepa5HHulqlxpYmleKaeKFq4kKcsl9z8zvj89vZ2dBYECGGd8+H3/JZz4z82IY8enn\nGkWv1wsAAAA8+GwsPQAAAADqBmEHAAAgCcIOAABAEoQdAACAJAg7AAAASRB2AAAAkiDsAAAA\nJEHYATCjixcvKoqiKMqZM2fM8fhr167t0KGDg4ODi4vL6tWrzfEU5Rw7dkz9jq5fv64u+eWX\nX9QliYmJla1Tbyz41ACsAWEHoGbKyso2b948YcKE9u3bu7u729nZeXp6duvWbfbs2cePH6/P\nSU6cODFlypRLly45Ojq2b99eo9FUtqa7u7uiKNHR0VU82oEDB9Qk+uWXX8wwLADUB1tLDwDg\nQXLgwIHp06dfu3bNeOHdu3fv3r17+vTp5cuXh4eHr1mzxsvLqx6G2bFjhxDCy8vr4sWLjRo1\nMsdT7Nmz5/HHH1+3bt2kSZPUJYGBgfHx8UKIZs2aVXYvU9ax4HgAJMYWOwCm2rhx4/Dhw69d\nu9agQYO5c+ceP348Oztbp9OlpqZu27atf//+Qog9e/aEhobm5OTUwzwpKSlCiJCQEDNVnRDi\n559/LrfE2dk5ODg4ODjY3t6+snuZso4FxwMgMcIOgEnOnj07bdo0rVYbEBDw22+/vf/++z16\n9GjYsKGiKN7e3mPHjv3xxx8XLVokhEhISHj55ZfrYSStViuEsLOzM9