blue\") +\n","    geom_point(aes(x = Arrival, y = Customer), color = \"green\") +\n","    geom_point(aes(x = End, y = Customer), color = \"red\") +\n","    labs(x = \"Time (minutes)\", y = \"Customer\", title = \"Customer Flow in Restaurant\", subtitle = \"Completed by: MZ & SJA\")\n","}\n","\n","plot_simulation(simulation_result)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":437},"id":"HwxmAXGv_VVq","executionInfo":{"status":"ok","timestamp":1732944167725,"user_tz":300,"elapsed":448,"user":{"displayName":"Mohammad Zoraiz","userId":"07318812600179621858"}},"outputId":"9ef7a251-5068-4d53-cf25-e1c7ba61adc3"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["plot without title"],"image/png":"iVBORw0KGgoAAAANSUhEUgAAA0gAAANICAIAAAByhViMAAAACXBIWXMAABJ0AAASdAHeZh94\nAAAgAElEQVR4nOzdZ2AU5dYH8DN9drak0ruAFEGkiHREiFQFLFhQ+hVRfEWFC1zkgihFRK+K\nWOhcUVBQIoKIXEFAL3oBQVABO2CoIdkk23dn5v0wsG5LSEJZdvn/vsiePTt7nidDOE57GF3X\nCQAAAAASHxvvAgAAAADg4kBjBwAAAJAk0NgBAAAAJAk0dgAAAABJAo0dAAAAQJJAYwcAAACQ\nJNDYAQAAACQJNHZXtaeffpphmNdee+1yfumECRMYhnnzzTcv55cCAABcDRKysTt16tQzzzzT\ntm3bChUqSJJUvXr1du3azZkz5/Tp05fi695+++21a9deii1fOnv37mVKNGbMmHjVlpaWVqtW\nLavVeiEbKW6AiqI0bNjw0Ucf/eWXXy5WwaEScWeghC0bAADKikm4lSf+/e9/jxo1yuVyCYLQ\nrFmztLS0nJycX375xefzpaWlrVy58tZbb72431ilSpXevXsvXLjw4m72ktq7d2/z5s3NZnO3\nbt1iJtx2223Dhw9/+umnp0+fPnfu3NGjR1/mCi+QMUCLxdK7d+9gUNf1kydP7tu3Lz8/32Qy\nrV27trjhl1si7gyUsGUDAEBZ8fEuoGxWrlw5ePBglmUnTZr097//3WazGfFTp05Nnz791Vdf\n7dWr186dO5s3b36xvvG33347ceLExdraZVa5cuXs7Ox4V3EJVapUaeXKlRFBl8s1duzYN954\nY/jw4b///jvLXrTD0gm6MyRo2QAAUA6JdCq2qKho1KhRRDRv3rznnnsu2NURUcWKFV955ZXx\n48erqhq8eGv06NEMwyxdujR0I19//TXDMH369AlGVq9efcstt6Snp4uiWLVq1Z49e27YsMF4\n66677qpbty4RLVq0iGGYDh06GHG/3//KK6/ceOONVqtVluV69eqNHj362LFjwW1OnjyZYZi1\na9fu2LHj5ptvtlqtFSpUGDJkSFFRka7rL7/8csOGDRVFady48cyZM0MPmuq6vnDhwrZt21qt\nVpPJ1KhRo8mTJzudzmDCpEmTjC2//vrr1apVS01NvTiTe07JQ6tWrZrJZPL5fMH8nTt3GudA\nc3JygsHTp0+zLHvjjTfG/IqIa+yCc/XDDz/079+/YsWKsizfcMMNK1asKN